{
    "info": {
        "_postman_id": "dotra-api-collection",
        "name": "Dotra API",
        "description": "API collection for Dotra application with Customer and Vendor authentication",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "variable": [
        {
            "key": "base_url",
            "value": "http://127.0.0.1:8000",
            "type": "string"
        },
        {
            "key": "customer_token",
            "value": "",
            "type": "string"
        },
        {
            "key": "vendor_token",
            "value": "",
            "type": "string"
        }
    ],
    "item": [
        {
            "name": "Verification",
            "item": [
                {
                    "name": "Confirm",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Accept",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/confirm",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "confirm"
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Process",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Accept",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/process",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "process"
                            ]
                        }
                    },
                    "response": []
                }
            ]
        },
